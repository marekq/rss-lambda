{
  "version" : "2017-02-28",
  "operation" : "Query",
  "index" : "timest",
  "query" : {
    "expression" : "blogsource = :bs and timest > :ts",
    "expressionValues" : {
      ":bs" : {"S": "$context.arguments.blogsource"},
      ":ts" : {"N": $context.arguments.timest}
    }
  },
  "nextToken": #if( $context.args.nextToken ) "$context.args.nextToken" #else null #end
}